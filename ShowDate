package homework3;

public class ShowDate {
	public static void main(String[] args) {
		java.util.Date date = new java.util.Date();  // 创建Date类的引用变量date
		long time = 10000;  // 设置第一个时间值为10000ms
		for(int i = 0; i < 8; i++) {  // 循环为date赋值，并显示日期
			date.setTime(time);
			System.out.println(date.toString());
			time *= 10;
		}
	}
}
